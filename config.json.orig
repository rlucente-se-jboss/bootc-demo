{
  "blueprint": {
    "customizations": {
      "user": [
        {
          "name": "${EDGE_USER}",
          "password": "${EDGE_HASH}",
          "key": "${SSH_PUB_KEY}",
          "groups": [
            "wheel"
          ]
        }
      ],
      "firewall": {
        "services": {
          "enabled": [
            "ssh",
            "http"
          ]
        }
      },
      "kernel": {
       "append": "fips=1"
      }
    }
  }
}
